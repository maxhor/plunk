{
	"version": "0.2.0",
	"configurations": [
		{
			
            
            "name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
            "file": "index.html",
            "runtimeArgs":["--allow-file-access-from-files"],
            "sourceMaps":true,
            "diagnosticLogging": false,
            // "outDir":null,
            // "address":"localhost",
            // "program":null,
            // "stopOnEntry":false,
            // "args":null,
            "cwd": "${workspaceRoot}",
            "runtimeExecutable":null
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost/mypage.html",
			"sourceMaps": true
		
		},
		{
            "name": "Chrome Attach",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "diagnosticLogging": true
        }

	]
}